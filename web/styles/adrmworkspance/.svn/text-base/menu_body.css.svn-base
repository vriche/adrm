#wrapper {
        border: 0px;
        margin: 0px;
        margin-left: auto;
        margin-right: auto;
        padding: 0px;
}

#header {
        background-color: #FFF;
        background-position: right top;
        border-bottom: 4px solid #80B5D7;
}

#mambo {
        position: relative;
        width: 100%;
        background: url(../../image/menu/header_blue.jpg) #30569D no-repeat;
        background-position: top right;
        margin: 0px;
        padding: 0px;
}

#wrapper1 div {
    border: 0px;
    margin: 0px;
    margin-left: auto;
    margin-right: auto;
    padding: 0px 5px 0px 5px;
    display: inline;
}

BODY {
	margin: 0px 0px 0px 0px;
	color : #333;
	background-color: #FFF;
	font-family: Tahoma, 宋体, Arial, Helvetica, sans-serif;
	font-size: 12px;
}
a {
	color: #ff6600;
	text-decoration: none;
}
a:hover {
	color : #30569D;
	text-decoration : underline;
}
a:active {
	color : #FF9900;
	text-decoration : underline;
}
a.toolbar {
	color : #808080;
	text-decoration : none;
}
a.toolbar:hover {
	color : #FF6600;
}
a.toolbar:active {
	color : #FF9900;
}
.pagenav {
	font-size : 12px;
}
a.pagenav {
	color : #808080;
	font-size : 12px;
	text-decoration : underline;
}
a.pagenav:hover {
	color : #FFF;
	font-size : 12px;
}
a.pagenav:active {
	color : #333;
	font-size : 12px;
}
.sectionname {
	color : #FF9900;
	font-family: Tahoma, 宋体, Arial, Helvetica, sans-serif;
	font-size : 18px;
	font-weight: bold;
}
.button {
	border-top : solid 1px #d5d5d5;
	border-right : solid 1px #808080;
	border-bottom : solid 1px #808080;
	border-left : solid 1px #d5d5d5;
	color : #333;
	font-weight : bold;
}
.inputbox {
        color: blue;
        background: #E9EFF5;
        font-family: Tahoma, 宋体, Courier, Helvetica, sans-serif;
        z-index: -3;
        font-size: 12px;
}
.text_area {
        border : 1px solid #999;
        color: blue;
        background: #E9EFF5;
        font-family: Tahoma, 宋体, Courier, Helvetica, sans-serif;
        z-index: -3;
        font-size: 12px
}
INPUT, TEXTAREA, SELECT {
	color : blue;
	z-index : -3;
	font-size: 12px;
}
.small {
	color : #FF9900;
	font-family: Tahoma, 宋体, Arial, Helvetica, sans-serif;
	font-size : 12px;
	text-decoration : none;
}
.smallgrey {
	color : #999;
	font-size : 12px;
}
.error {
	color : #C40000;
	font-size : 12px;
	font-weight : bold;
}
.contentheading, .componentheading, .moduleheading {
	font-size: 14px;
	font-weight: bold;
}
table.menubar {
	width: 100%;
	
}
td {
	font-size: 12px;
}
/* header block */
table.adminheading {
	background-color: #FFF;
	font-family: Tahoma, 宋体, Arial, Helvetica, sans-serif;
	margin: 0px;
	padding: 0px;
	border: 0px;
	width: 100%;
	border-collapse: collapse;
	color: #FF9E31;
	font-size : 18px;
	font-weight: bold;
	text-align: left;
}
table.adminheading th {
	width: 99%;
	height: 50px;
	padding-left: 50px;
	background: url(../../image/menu/asterisk.png) no-repeat left;
}
table.adminheading th.checkin {
	background: url(../../image/menu/checkin.png) no-repeat left;
}
table.adminheading th.cpanel {
	background: url(../../image/menu/cpanel.png) no-repeat left;
}
table.adminheading th.config {
	background: url(../../image/menu/config.png) no-repeat left;
}
table.adminheading th.modules {
	background: url(../../image/menu/module.png) no-repeat left;
}
table.adminheading th.menus {
	background: url(../../image/menu/menu.png) no-repeat left;
}
table.adminheading th.trash {
	background: url(../../image/menu/trash.png) no-repeat left;
}
table.adminheading th.user {
	background: url(../../image/menu/user.png) no-repeat left;
}
table.adminheading th.inbox {
	background: url(../../image/menu/inbox.png) no-repeat left;
}
table.adminheading th.msgconfig {
	background: url(../../image/menu/message_config.png) no-repeat left;
}
table.adminheading th.langmanager {
	background: url(../../image/menu/langmanager.png) no-repeat left;
}
table.adminheading th.mediamanager {
	background: url(../../image/menu/mediamanager.png) no-repeat left;
}
table.adminheading th.impressions {
	background: url(../../image/menu/impressions.png) no-repeat left;
}
table.adminheading th.browser {
	background: url(../../image/menu/browser.png) no-repeat left;
}
table.adminheading th.searchtext {
	background: url(../../image/menu/searchtext.png) no-repeat left;
}
table.adminheading th.templates {
	background: url(../../image/menu/templatemanager.png) no-repeat left;
}
table.adminheading th.massemail {
	background: url(../../image/menu/massemail.png) no-repeat left;
}
table.adminheading th.frontpage {
	background: url(../../image/menu/frontpage.png) no-repeat left;
}
table.adminheading th.sections {
	background: url(../../image/menu/sections.png) no-repeat left;
}
table.adminheading th.edit {
	background: url(../../image/menu/addedit.png) no-repeat left;
}
table.adminheading th.categories {
	background: url(../../image/menu/categories.png) no-repeat left;
}
table.adminheading th.install {
	background: url(../../image/menu/install.png) no-repeat left;
}
table.adminheading th.dbbackup {
	background: url(../../image/menu/backup.png) no-repeat left;
}
table.adminheading th.dbrestore {
	background: url(../../image/menu/dbrestore.png) no-repeat left;
}
table.adminheading th.dbquery {
	background: url(../../image/menu/query.png) no-repeat left;
}
table.adminheading th.info {
	background: url(../../image/menu/systeminfo.png) no-repeat left;
}

table.adminheading td {
	color : #000;
}
/* standard list style table */
table.adminlist {
	background-color: #FFFFFF;
	margin: 0px;
	padding: 0px;
	border: 1px solid #CCCCCC;
	border-spacing: 0px;
	width: 100%;
	border-collapse: collapse;
}

table.adminlist th {
	margin: 0px;
	padding: 6px 4px 2px 4px;
	height: 25px;
	background: url(../../image/menu/background.gif);
	background-repeat: repeat;
	font-size: 12px;
	color: #ffffff;
}
table.adminlist th.title {
	text-align: left;
}

table.adminlist th a {
	color: #ff9900;
}

table.adminlist tr.row0 {
	background-color: #F5F5F5;
}
table.adminlist tr.row1 {
	background-color: #FFF;
}
table.adminlist td {
	border-bottom: 1px solid #e5e5e5;
	padding: 4px;
}
table.adminlist tr.row0:hover {
	background-color: #f1f1f1;
}
table.adminlist tr.row1:hover {
	background-color: #f1f1f1;
}
table.adminlist td.options {
	background-color: #ffffff;
	font-size: 12px;
}
select.options, input.options {
	font-size: 12px;
	font-weight: normal;
	border: 1px solid #999999;
}
/* standard form style table */
table.adminform {
	background-color: #f5f5f5;
	border: solid 1px #d5d5d5;
	width: 100%;
	padding: 10px;
	border-collapse: collapse;
}
table.adminform tr.row0 {
	background-color: #f5f5f5;
}
table.adminform tr.row1 {
	background-color: #eeeeee;
}
table.adminform th {
	font-size: 12px;
	padding-top: 6px;
	padding-bottom: 2px;
	padding-left: 4px;
	padding-right: 4px;
	text-align: left;
	height: 25px;
	color: #ffffff;
	background: url(../../image/menu/background.gif);
	background-repeat: repeat;
}
table.adminform td {
	padding: 3px;
	text-align: left;
}
table.adminform td.editor {
	font-size: 12px;
}

table.cpanel td {
	text-align: center;
}

form {
	margin: 0px 0px 0px 0px;
}
/* Styles for dhtml tabbed-pages */
.ontab {
	font-size: 12px;
	background-color: #ffae00;
	border: 1px solid #cccccc;
	width: 14%;
	text-align: center;
	cursor: hand;
	font-weight: bold;
	color: #FFFFFF;
}
.offtab {
	font-size: 12px;
	background-color : #e5e5e5;
	border: 1px solid #cccccc;
	width: 14%;
	text-align: center;
	cursor: hand;
	font-weight: normal;
}
.tabpadding {
	/*border-bottom: solid 1px #D5D5D5;*/
}
.tabheading {
	background-color: #FFAE00;
	color: #FFF;
	font-size: 12px;
	text-align: left;
}
.pagetext {
	visibility: hidden;
	display: none;
	position: relative;
	top: 0;
}
.menubackgr {
	/* background: #F1F3F5; */
	border-bottom: 1px solid #cccccc;
	background: ThreeDFace;
	/* background: ThreeDFace; */

}
.menudottedline {
	border-bottom: 1px solid #cccccc;
	border-top: 1px solid #ffffff;
	background-color: #F1F3F5;
}
.pathway {
	border: 1px inset;
	background-color: #FFF;
	padding: 2px;
	width: 95%;
	margin: 3px;
}
.adminheader {
	color : #FF9900;
	font-size : 24px;
}
/* Text passed with mosmsg url parameter */
.message {
	font-family: Tahoma, 宋体, "Trebuchet MS", Arial, Helvetica, sans-serif;
	font-weight: bold;
	font-size : 14px;
	color : #FF9900;
	text-align: center;
}
.main {
	border: 1px solid #d5d5d5;
	width: 98%;
}
.quote {
	margin: 4px;
	border: 1px solid #cccccc;
	background-color: #E9ECEF;
	padding: 10px;
	font-size: 12px;
	color: #254D78;
}
