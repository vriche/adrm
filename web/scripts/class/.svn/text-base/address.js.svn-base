
function Address(){
	//创建对象
	this.obj ={
		address:null,
		city:null,
		province:null,
		country:null,
		postalCode:null
	}
	return this.obj;
}

//清空对象
Address.prototype.reset = function(){
	this.obj.address = null;
	this.obj.city = null;
	this.obj.province = null;
	this.obj.country = null;
	this.obj.postalCode = null;
}